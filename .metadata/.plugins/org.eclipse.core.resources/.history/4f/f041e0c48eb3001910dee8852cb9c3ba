
package fellowship; 
import java.util.ArrayList;
import java.util.Arrays;
public class Demo {		
		 public static void main(String args[]){ 
			 int x=17; 
			 int a[]=new int[3];
				a[0]=x%10;
				x=x/10;
				a[1]=x%10;
				x=x/10;
				a[2]=x%10;
				
				for(int i=0;i<3;i++)//length is the property of array  
					System.out.println(a[i]);
		 }
  
}